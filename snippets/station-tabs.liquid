{%- capture header_content -%}{{- content_for_header -}}{%- endcapture -%}
{%- if header_content contains 'tabs-by-station.js' -%}
  {%- capture tabs_order -%}{%- include 'station-tabs-order' -%}{%- endcapture -%}
  
  {%- if template.name == 'product' -%}
    <div id="station-tabs-data">
      <!-- begin station data -->
      {% capture tabs_data %}{% include 'station-tabs-data', tabs_order: tabs_order %}{% endcapture %}
      {%- if tabs_data contains 'station-tabs-data' -%}{{ tabs_data }}{%- endif -%}

      <script id="station-tabs-product-json" type="application/json">{"productDescriptionHTML": {{ product.description | json }}}</script>
      <!-- end station data -->
    </div>
  {%- endif -%}
  

